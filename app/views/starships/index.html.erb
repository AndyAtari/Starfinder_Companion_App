<h1>STARSHIPS</h1>

<table class="table">
<tr>
<th>Name</th>
<th>Tier</th>
<th>Status</th>
</tr>


<%@starships.each do |starship|%>
<tr>
    <td><%=starship.name%></td>
    <td><%= starship.tier%></td>
    <td><%= link_to "Update/Delete", edit_starship_path(starship)%></td>
</tr>
<%end%>
</table>



