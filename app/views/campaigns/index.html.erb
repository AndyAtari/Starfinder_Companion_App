<h1>YOUR CAMPAIGNS</h1>

<table class="table">
<tr>
<th>Campaign Title</th>
<th>Adventure Path</th>
<th>Game Master</th>
<th>Looking For Players</th>
<th>Characters</th>
<th>Status</th>
</tr>

<% @campaign.each do |campaign|%>
    <tr>
        <td><%= campaign.title%></td>
        <td><%= campaign.adventure_path%></td>
        <td><%= campaign.game_master%></td>
        <td><%= campaign.need_players?%></td>
        <td><%= link_to "Character Name"%></td>
        <td><%= link_to "Update/Delete"%></td>
    </tr>
<%end%>

</table>